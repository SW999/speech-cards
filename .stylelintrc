{
  "extends": "stylelint-config-recommended",
  "plugins": [
    "stylelint-prettier",
    "stylelint-scss"
  ],
  "rules": {
    "prettier/prettier": null,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "block-no-empty": null,
    "unit-allowed-list": ["em", "rem", "s", "px", "%", "deg", "vh", "vw"],
    "no-descending-specificity": null
  }
}
